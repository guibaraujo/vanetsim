# -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):

    obj = bld.create_ns3_program('vndn-example-beacon', ['ndn4ivc'])
    obj.source = 'vndn-example-beacon.cc'

    obj = bld.create_ns3_program('vndn-example-tms', ['ndn4ivc'])
    obj.source = 'vndn-example-tms.cc'

    obj = bld.create_ns3_program('vndn-example-cbr', ['ndn4ivc'])
    obj.source = 'vndn-example-cbr.cc'   
